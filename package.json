{
  "name": "koapi-starter",
  "version": "0.0.2",
  "description": "This is a starter for KoaJS API",
  "main": "server.js",
  "author": "Louisloode",
  "contributors": [
    {
      "name": "Louis DEBRAINE",
      "email": "louisdebraine@gmail.com"
    }
  ],
  "private": false,
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha 'test/**/*.js' --reporter spec",
    "start": "NODE_ENV=production pm2 start process.json",
    "stop": "pm2 stop process.json && pm2 delete process.json"
  },
  "keywords": [
