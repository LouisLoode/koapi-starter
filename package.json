    "passport-local": "~1.0.0",
    "passport-localapikey": "0.0.3",
    "pm2": "~1.1.3",
    "winston": "~2.1.1"
  },
  "devDependencies": {
    "apidoc": "~0.16.1",
    "expect.js": "~0.3.1",
    "faker": "~3.0.1",
    "mocha": "3.0.2",
    "nodemon": "~1.9.2",
    "should": "~11.1.0",
    "supertest": "2.0.0"
  }
}
