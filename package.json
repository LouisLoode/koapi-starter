    "KoaJS",
    "NodeJS",
    "API"
  ],
  "dependencies": {
    "bcrypt": "~0.8.7",
    "boom": "~3.2.2",
    "co": "~4.6.0",
    "include-all": "~0.1.6",
    "joi": "~9.0.4",
    "koa": "~1.2.0",
    "koa-boom": "0.0.4",
    "koa-cors": "0.0.16",
    "koa-csrf": "~2.5.0",
    "koa-generic-session": "~1.11.3",
    "koa-joi-router": "~4.0.0",
    "koa-jwt": "~1.2.0",
    "koa-logger": "~1.3.0",
    "koa-passport": "~1.3.1",
    "koa-redis": "~2.1.2",
    "koa-response-time": "~1.0.2",
    "koa-serve": "~0.1.7",
    "kue": "~0.11.1",
    "lodash": "~4.15.0",
    "mongoose": "~4.5.9",
    "passport-http-bearer": "~1.0.1",
    "passport-http-jwt-bearer": "~0.1.3",
